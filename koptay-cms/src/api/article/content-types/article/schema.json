{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "excerpt": {
      "type": "text",
      "required": true
    },
    "category": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "Ceza Hukuku",
        "Aile Hukuku",
        "Ticaret Hukuku",
        "İş Hukuku",
        "İcra-İflas",
        "İdare Hukuku"
      ]
    },
    "author": {
      "type": "string",
      "default": "Av. Murat Can Koptay",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "seoTitle": {
      "type": "string"
    },
    "seoDescription": {
      "type": "text"
    },
    "keywords": {
      "type": "string"
    },
    "readTime": {
      "type": "integer",
      "required": true
    },
    "publishedat": {
      "type": "date",
      "required": true
    },
    "article": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::article.article",
      "private": true,
      "inversedBy": "articles"
    },
    "articles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::article.article",
      "private": true,
      "mappedBy": "article"
    }
  }
}
